{
  "name": "photopealoader",
  "productName": "Photopea",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-linux": "electron-packager . --icon icons/linux.png --platform linux",
    "build-darwin": "electron-packager . --icon icons/darwin.icns --platform darwin",
    "build-win32": "electron-packager . --icon icons/win32.ico --platform win32"
  },
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.5"
  }
}
